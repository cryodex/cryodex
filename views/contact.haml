%section#main{:role => "main"}
  .breadcrumb-container
    .row
      .large-12.column
        %nav.breadcrumbs.animated.bounceInDown
          %a{:href => "index.html"} Home
          %a.current{:href => "#"} Contact Us
  %section.part
    .row.title
      .large-12.column
        %h6 How to reach us
    .row
      .large-7.column
        %p
